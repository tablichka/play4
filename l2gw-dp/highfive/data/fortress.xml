<forts>
	<!-- Common settings for all forts -->
	<common>
		<!-- (in minutes) Default: 50 min time to register for fort -->
		<Registration time="50"/>
		<!-- (in minutes) Default: 10 min Countdown after registration time is over -->
		<Countdown time="10"/>
		<!-- (in minutes) Default: 1 hour fortress siege time -->
		<SiegeLength time="60"/>
		<!-- (in hours) Default: 75 hours you can own a fort  -->
		<Hold time="168"/>
		<!-- (in hours) Default: 2 hours untill rebel army takes fort back -->
		<Rebel time="2"/>
		<!-- CRP reward for taking a fortress if Clan is level 5 or higher -->
		<CRPWin val="200"/>
		<!-- Amount of Fame points awarded to players in siege zone each 5 min -->
		<FamePoints val="31"/>
		<!-- Default: 4 hours interval between siege for the same fortress -->
		<NextSiegePeriod time="4"/>
		<!-- Default: Minimum Clan Level to register -->
